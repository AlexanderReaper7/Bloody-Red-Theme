<?xml version="1.0" encoding="utf-8"?>
<animated-selector
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@+id/enableCheck" android:drawable="@*android:drawable/jog_tab_bar_left_answer"
        android:state_checked="true" android:state_enabled="true"/>
    <item android:id="@+id/disableCheck" android:drawable="@*android:drawable/jog_tab_bar_left_generic"
        android:state_checked="true" android:state_enabled="false"/>
    <item android:id="@+id/enableUncheck" android:drawable="@*android:drawable/jog_tab_bar_left_unlock"
        android:state_checked="false" android:state_enabled="true"/>
    <item android:id="@+id/disableUncheck" android:drawable="@*android:drawable/jog_tab_bar_right_decline"
        android:state_checked="false" android:state_enabled="false"/>

    <transition android:fromId="@+id/enableCheck" android:toId="@+id/enableUncheck">
        <animation-list>
            <item android:duration="30" android:drawable="@*android:drawable/jog_tab_bar_left_answer"/>
            <item android:duration="30" android:drawable="@*android:drawable/jog_tab_bar_right_sound_on"/>
            <item android:duration="30" android:drawable="@*android:drawable/jog_tab_bar_right_generic"/>
            <item android:duration="30" android:drawable="@*android:drawable/jog_tab_bar_right_sound_off"/>
            <item android:duration="30" android:drawable="@*android:drawable/jog_tab_bar_left_unlock"/>
        </animation-list>
    </transition>

    <transition android:fromId="@+id/enableUncheck" android:toId="@+id/enableCheck">
        <animation-list>
            <item android:duration="30" android:drawable="@*android:drawable/jog_tab_bar_left_unlock"/>
            <item android:duration="30" android:drawable="@*android:drawable/jog_tab_bar_right_sound_off"/>
            <item android:duration="30" android:drawable="@*android:drawable/jog_tab_bar_right_generic"/>
            <item android:duration="30" android:drawable="@*android:drawable/jog_tab_bar_right_sound_on"/>
            <item android:duration="30" android:drawable="@*android:drawable/jog_tab_bar_left_answer"/>
        </animation-list>
    </transition>
</animated-selector>