<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.DeskClock" parent="@*com.google.android.deskclock:style/Theme.AppCompat.NoActionBar">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:navigationBarColor">@android:color/holo_blue_dark</item>
        <item name="android:timePickerStyle">@*com.google.android.deskclock:style/Widget.TimePicker</item>
        <item name="*com.google.android.deskclock:popupTheme">@*com.google.android.deskclock:style/ThemeOverlay.Popup</item>
        <item name="*com.google.android.deskclock:imageButtonStyle">@*com.google.android.deskclock:style/Widget.ImageButton</item>
        <item name="*com.google.android.deskclock:colorPrimaryDark">#66000000</item>
        <item name="*com.google.android.deskclock:colorAccent">@android:color/holo_blue_bright</item>
        <item name="*com.google.android.deskclock:colorControlNormal">?android:textColorPrimary</item>
        <item name="*com.google.android.deskclock:colorControlActivated">@android:color/white</item>
        <item name="*com.google.android.deskclock:colorBackgroundFloating">@android:color/holo_blue_light</item>
    </style>
</resources>
